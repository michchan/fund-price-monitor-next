{
  "name": "fund-price-monitor-next",
  "version": "0.12.2",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0"
  },
  "scripts": {
    "dev": "concurrently -n next,tsm \"next dev\" \"yarn tsm-watch\"",
    "prebuild": "ts-node --show-config && ts-node -r tsconfig-paths/register -r dotenv/config scripts/generateNoLangFallbackPages",
    "build": "next build && next export",
    "postbuild": "node scripts/removeNoLangFallbackPages",
    "start": "serve out",
    "deploy": "node scripts/deployToS3",
    "deploy-local": "node -r dotenv/config scripts/deployToS3 dotenv_config_path=.env.local",
    "link": "yarn link \"@michchan/fund-price-monitor-lib\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint-fix": "yarn lint --fix",
    "sync-i18n-json": "node scripts/syncI18nJSONs",
    "tsm": "tsm src --nameFormat none",
    "tsm-watch": "yarn tsm -w",
    "tsc-watch": "tsc --watch --noemit",
    "release": "standard-version --ignore-path .gitignore --commit-hook false"
  },
  "dependencies": {
    "@michchan/fund-price-monitor-lib": "0.13.1",
    "chart.js": "^4.4.2",
    "dayjs": "^1.10.5",
    "html-react-parser": "^1.2.4",
    "http-proxy-agent": "^4.0.1",
    "https-proxy-agent": "^5.0.0",
    "lodash": "^4.17.21",
    "next": "12.1.0",
    "next-i18next": "10.5.0",
    "nextjs-progressbar": "^0.0.11",
    "node-fetch": "2.6.1",
    "nprogress": "^0.2.0",
    "qs": "^6.10.1",
    "react": "17.0.2",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "17.0.2",
    "react-select": "^4.3.1",
    "sass": "^1.32.8",
    "simply-utils": "0.63.2"
  },
  "devDependencies": {
    "@michchan/eslint-config": "1.11.0",
    "@types/chart.js": "^2.9.41",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.6.1",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.2",
    "@types/react-select": "^4.0.16",
    "aws-sdk": "^2.884.0",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "7.32.0",
    "mime-types": "^2.1.29",
    "serve": "^11.3.2",
    "standard-version": "^9.3.2",
    "ts-node": "10.7.0",
    "tsconfig-paths": "^3.9.0",
    "typed-scss-modules": "^4.1.0",
    "typescript": "4.6.2"
  }
}
