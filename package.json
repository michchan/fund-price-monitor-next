{
  "name": "fund-price-monitor-next",
  "version": "0.2.3",
  "private": true,
  "scripts": {
    "build": "yarn generate-no-lang-fallback-pages && next build && next export && yarn remove-no-lang-fallback-pages",
    "deploy": "node scripts/deployToS3",
    "deploy-local": "node -r dotenv/config scripts/deployToS3 dotenv_config_path=.env.local",
    "dev": "concurrently -n next,tsm \"next dev\" \"yarn tsm-watch\"",
    "generate-no-lang-fallback-pages": "ts-node -r tsconfig-paths/register --project tsconfig.script.json scripts/generateNoLangFallbackPages",
    "remove-no-lang-fallback-pages": "node scripts/removeNoLangFallbackPages",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint-fix": "yarn lint --fix",
    "start": "serve out",
    "sync-i18n-json": "node scripts/syncI18nJSONs",
    "tsm": "tsm src --nameFormat none",
    "tsm-watch": "yarn tsm -w",
    "tsc-watch": "tsc --watch --noemit"
  },
  "dependencies": {
    "html-react-parser": "^1.2.4",
    "lodash": "^4.17.21",
    "next": "10.0.7",
    "next-i18next": "^8.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.8",
    "simply-utils": "^0.55.0"
  },
  "devDependencies": {
    "@michchan/eslint-config": "^1.8.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "aws-sdk": "^2.870.0",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "mime-types": "^2.1.29",
    "serve": "^11.3.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typed-scss-modules": "^4.1.0",
    "typescript": "^4.2.2"
  }
}
