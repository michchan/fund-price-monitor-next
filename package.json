{
  "name": "fund-price-monitor-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build && next export",
    "dev": "concurrently -n next,tsm \"next dev\" \"yarn tsm-watch\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint-fix": "yarn lint --fix",
    "start": "serve out",
    "sync-i18n-json": "node scripts/syncI18nJSONs",
    "tsm": "tsm src",
    "tsm-watch": "yarn tsm -w",
    "watch-tsc": "tsc --watch --noemit"
  },
  "dependencies": {
    "http-status-codes": "^2.1.4",
    "next": "10.0.7",
    "next-i18next": "^8.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "@michchan/eslint-config": "^1.8.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "serve": "^11.3.2",
    "typed-scss-modules": "^4.1.0",
    "typescript": "^4.2.2"
  }
}
